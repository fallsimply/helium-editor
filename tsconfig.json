{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": false,
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowJs": true,

		"lib": [
			"esnext",
			"dom"
		],
		"baseUrl": "."
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
	]
}