{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"strict": false,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowJs": true,
		"lib": [
			"esnext",
			"dom"
		],
		"types": [
			"vite/client",
			"@types/node"
		],
		"baseUrl": "."
	},
	"include": [
		"**/*.ts",
		"**/*.d.ts"
	]
}